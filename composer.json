{
    "name": "Mednav.ru source code",
    "description": "Web portal based on yii2 framework",
    "keywords": ["yii2", "mednav.ru"],
    "homepage": "http://www.mednav.ru/",
    "type": "project",
    "support": {
        "issues": "https://github.com/pa3py6aka/mednav/issues?state=open",
        "source": "https://github.com/pa3py6aka/mednav"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "pa3py6aka/yii2-modal-alert-widget": "^1.2",
        "yiisoft/yii2-redis": "~2.0.0",
        "paulzi/yii2-nested-sets": "^1.0",
        "mihaildev/yii2-ckeditor": "*",
        "yii2tech/filedb": "^1.0",
        "yiisoft/yii2-imagine": "^2.1",
        "zelenin/yii2-slug-behavior": "~1.5.1",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "kartik-v/yii2-widget-datepicker": "dev-master",
        "yiisoft/yii2-jui": "^2.0",
        "bower-asset/fancytree": ">=2.28",
        "yiisoft/yii2-queue": "^2.1",
        "spatie/image-optimizer": "^1.1",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "himiklab/yii2-recaptcha-widget": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.4.0",
        "codeception/verify": "~0.3.1",
        "codeception/codeception": "^2.5"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
